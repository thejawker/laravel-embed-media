{
  "name": "thejawker/laravel-embed-media",
  "description": "associate embeddable content with a Laravel Model.",
  "keywords": [
    "thejawker",
    "laravel-embed-media"
  ],
  "homepage": "https://github.com/thejawker/laravel-embed-media",
  "license": "MIT",
  "authors": [
    {
      "name": "bram Veerman",
      "email": "info@thejawker.com",
      "homepage": "https://thejawker.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "illuminate/support": "~5.6|~5.7",
    "kitetail/zttp": "^0.3.0",
    "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
    "madcoda/php-youtube-api": "^1.2"
  },
  "require-dev": {
    "larapack/dd": "^1.0",
    "orchestra/testbench": "^3.7",
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "classmap": [
      "database/migrations"
    ],
    "psr-4": {
      "TheJawker\\LaravelEmbedMedia\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TheJawker\\LaravelEmbedMedia\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "TheJawker\\LaravelEmbedMedia\\EmbedMediaServiceProvider"
      ]
    }
  }
}
